<Window x:Class="DaikyuViewer.EditPass"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="パスワード変更" Height="239" Width="370" WindowStartupLocation="CenterScreen" ResizeMode="CanMinimize">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="54*" />
            <ColumnDefinition Width="291*" />
        </Grid.ColumnDefinitions>
        <Label Height="32" HorizontalAlignment="Left" Margin="48,59,0,0" Name="lPassword" VerticalAlignment="Top" Width="107" Content="新しいパスワード" Grid.ColumnSpan="2" />
        <Button Content="変更" Height="29" HorizontalAlignment="Left" Margin="62,130,0,0" Name="bEnter" VerticalAlignment="Top" Width="101" Click="bEnter_Click" TabIndex="3" Grid.Column="1" />
        <PasswordBox Height="23" HorizontalAlignment="Left" Margin="121,61,0,0" Name="pbPass" VerticalAlignment="Top" Width="117" TabIndex="2" Grid.Column="1" />
        <Label Content="※10文字以内でﾊﾟｽﾜｰﾄﾞを設定して下さい。" Foreground="Blue" Height="27" HorizontalAlignment="Left" Margin="48,14,0,0" Name="label1" VerticalAlignment="Top" Width="268" Grid.ColumnSpan="2" />
    </Grid>
</Window>
